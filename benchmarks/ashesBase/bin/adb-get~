#!/bin/bash

if [ ! $ASHES_DATABASE ]
then
   echo "Please define the ASHES_DATABASE environment variable to contain"
   echo "the path to the directory of set files."
   exit 0
fi


if [ $# != 2 ]
then
    echo "Syntax: adb-get <set> <key>"
    exit 0
fi

SET=$1
KEY=$2

VALUE=`awk "/${KEY}/" ${ASHES_DATABASE}/$SET | cut -f 2 -d =`
echo $VALUE

